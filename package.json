{
  "name": "@matthewbonig/nightynight",
  "description": "A CDK construct that will automatically stop a running EC2 instance at a given time.",
  "version": "0.1.0",
  "public": true,
  "bin": {
    "nightynight": "bin/nightynight.js"
  },
  "main": "lib/nightynight.js",
  "types": "lib/nightynight.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "tsc:build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "mbonig.nightynight",
        "module": "mbonig.nightynight"
      }
    }
  },
  "keywords": [
    "cdk",
    "ec2"
  ],
  "repository": {
    "url": "https://github.com/mbonig/nightynight",
    "type": "git"
  },
  "author": {
    "name": "Matthew Bonig",
    "email": "matthew.bonig@gmail.com",
    "url": "https://matthewbonig.com"
  },
  "awscdkio": {
    "twitter": "mattbonig"
  },
  "license": "MIT",
  "devDependencies": {
    "@aws-cdk/assert": "1.60.0",
    "@types/jest": "^26.0.4",
    "@types/node": "10.17.27",
    "jest": "^26.0.4",
    "ts-jest": "^26.1.3",
    "aws-cdk": "1.60.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.6",
    "jsii": "^1.11.0",
    "jsii-pacmak": "^1.11.0",
    "source-map-support": "^0.5.16"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.60.0",
    "@aws-cdk/aws-events": "^1.60.0",
    "@aws-cdk/aws-events-targets": "^1.60.0",
    "@aws-cdk/aws-iam": "^1.60.0",
    "@aws-cdk/aws-lambda": "^1.60.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.60.0",
    "@aws-cdk/core": "1.60.0"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-ec2": "^1.60.0",
    "@aws-cdk/aws-events": "^1.60.0",
    "@aws-cdk/aws-events-targets": "^1.60.0",
    "@aws-cdk/aws-iam": "^1.60.0",
    "@aws-cdk/aws-lambda": "^1.60.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.60.0",
    "@aws-cdk/core": "1.60.0",
    "constructs": "^3.0.4"
  }
}
